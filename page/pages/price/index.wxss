}
.cell-title .mue-button text{
  margin-right: 0;
}
.cell-title .icon-query{
  margin-left: 25rpx;
  margin-right: 10rpx;
}
.cell-title .cell-item-inner{
  margin: 0 20rpx;
}
.grid-col-3 .cell-item:last-child .cell-item-inner{
  border-bottom: none;
}
.cell-item-inner.self{
