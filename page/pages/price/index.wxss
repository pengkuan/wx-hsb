.modal-outter{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10001;
  background: rgba(0,0,0,.5);
  display: none;
}
.modal-outter.show{
  display: block;
}
.modal-box{
  position: fixed;
  bottom: -100%;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10002;
}
.modal-box .modal-body .closeBtn{
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  top: 200rpx;
  left: 25rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 10002;
}
.modal-box .modal-body .close{
  width: 22rpx;
  height: 22rpx;
}
.modal-box .modal-body .modal-content{
  height: 85%;
  width: 100%;
  box-sizing: border-box;
  padding: 0 40rpx;
  background-color: #fff;
  position: absolute;
  bottom: 0;
  border-radius: 16rpx 16rpx 0 0;
} 
.modal-content .header{
  width: 100%;
  text-align: center;
  margin-top: 27rpx;
  color: #292e3e;
  font-size: 32rpx;
}
.modal-content .content{
  margin-top: 60rpx;
} 
.modal-content .content view{
  box-sizing: border-box;
  float: left;
  text-align: center;
  font-size: 26rpx;
}
.modal-content .content .th{
  width: 100%;
  color: #292e3e;
  line-height: 80rpx;
}
.modal-content .content .c2{
  width: 100%;
  line-height: 80rpx;
  margin-top: 20rpx;
}
.modal-content .content .c2 .col-3{
  color: #8489a2;
}
.modal-content .content .tr{
  width: 100%;
  color: #8489a2;
  height: 180rpx;
}
.modal-content .content .tr view{
  display: flex;
  justify-content: center;
  align-items: center;
}
.modal-content .content .tr view text{
  width: 120rpx;
}
.modal-content .content .col-1{
  width: 25%;
  border: 1rpx solid #7c829d;
  border-right: none;
  border-bottom: none;
  height: 100%;
}
.modal-content .content .col-2{
  width: 50%;
  border: 1rpx solid #7c829d;
  border-right: none;
  border-bottom: none;
  height: 100%;
}
.modal-content .content .col-3{
  width: 75%;
  border: 1rpx solid #7c829d;
  border-right: none;
  border-bottom: none;
  height: 100%;
}
.modal-content .content .bb{
  border-bottom: 1rpx solid #7c829d;
}
.modal-content .content .br{
  border-right: 1rpx solid #7c829d;
}
.modal-content .content .c3{
  margin-top: 43rpx;
}
.modal-content .content .c3 .th{
  font-size: 28rpx;
  color: #292e3e;
  text-align: left;
}
.modal-content .content .c3 view{
  color: #7c829d;
  text-align: left;
  padding: 5rpx 0;
  line-height: 1.4;
}