.container {
   background: #fff; 
}
page{
  background: #fff; 
}

.price-result {
  background-color: #fff;
  position: relative;
  width: 690rpx;
  height: 240rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  font-size: 0;
  color: #ffffff;
  text-align: center;
}

.price-result-bg {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  border-radius: 16rpx;
}

.price-result-inner {
  z-index: 2;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: transparent;
}

.price-result .icon-money {
  display: block;
  width: 76rpx;
  height: 76rpx;
  margin: 0 auto;
  margin-top: 20rpx;
}

.price-wrapper {
  overflow: visible;
  position: relative;
  display: inline-block;
  margin-top: 16rpx;
  font-size: 0;
  line-height: 1;
}

.price-wrapper .label {
  position: absolute;
  top: 4rpx;
  left: -36rpx;
  font-size: 36rpx;
}

.price-wrapper .number {
  font-size: 66rpx;
  line-height: 1;
}

.price-desc {
  margin-top: 9rpx;
  font-size: 22rpx;
}

.select-info {
  padding: 0 40rpx;
}

.grid-col {
  display: flex;
  float: none;
  width: 100%;
  /* margin-top: 50rpx; */
  color: #292e3e;
  font-size: 32rpx;
  line-height: 1;
  align-items: center;
  justify-content: space-between;
}
.grid-col-1 .cell-item-inner{
  border-bottom: 1rpx solid #eff0f6;
}
.select-list {
  overflow: hidden;
  max-height: 70rpx;
  margin-top: 39rpx;
  transition: all 300ms;
}

.select-list.open {
  max-height: 1000rpx;
}

.select-item {
  float: left;
  width: 217rpx;
  height: 70rpx;
  padding:0 4rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  color: #aab0cb;
  font-size: 24rpx;
  text-align: center;
  line-height: 70rpx;
  background-color: #f9f9fb;
}

.mr-0 {
  margin-right: 0;
}

.next-predict {
  height: 70rpx;
  color: #7c829d;
  font-size: 26rpx;
  line-height: 70rpx;
  text-align: center;
  border-top: 1rpx solid #eff0f6;
  border-bottom: 1rpx solid #eff0f6;
}

.next-predict .arrow_down {
  width: 10rpx;
  height: 24rpx;
  color: #66d4bb;
}

.chart {
  margin-top: 22rpx;
  padding: 0 40rpx;
}

.chart-tit {
  display: flex;
  margin-top: 30rpx;
  align-items: center;
  justify-content: center;
}

.chart-tit .mine-price,
.chart-tit .mrk-price {
  display: inline-block;
  margin: 0 20rpx;
  color: #aab0cb;
  font-size: 22rpx;
  text-align: center;
}

.mrk-price .line,
.mine-price .line {
  width: 38rpx;
  height: 4rpx;
  margin: 0 auto;
  margin-bottom: 19rpx;
}

.mine-price .line {
  background: #fabe00;
}

.mrk-price .line {
  background: #b967ea;
}

.chart-wrapper {
  overflow: hidden;
  display: flex;
}

.chart-left {
  overflow: hidden;
  width: 90rpx;
}

.chart-right {
  overflow: hidden;
  flex: 1;
}

.chart-left .axis-y {
  display: flex;
  flex-direction: column;
}

.chart-left .axis-text {
  color: #cacee0;
  font-size: 24rpx;
  line-height: 64rpx;
}

.chart-right .axis-x {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-top: 1rpx solid #e7e8f0;
}

.chart-right .axis-text {
  padding-top: 20rpx;
  color: #cacee0;
  font-size: 24rpx;
  text-align: right;
}

.chart-right .mue-chart {
  height: 288rpx;
}

.chart-rights .canvas-img {
  width: 100%;
  height: 100%;
}

.plug-canvas {
  position: relative;
  z-index: 99;
  width: 690rpx;
  height: 400rpx;
  margin: 0 auto;
  margin-left:2%;
  box-sizing: border-box;
}

.mue-footer {
  z-index: 10000;
  position: fixed;
  bottom: 0;
  width: 100%;
  padding: 25rpx 40rpx;
  background-color: #ffffff;
  box-shadow: 0rpx 6rpx 24rpx 3rpx rgba(135, 142, 160, 0.15);
}

.mue-content {
  position: relative;
  z-index: 1;
  padding-bottom: 140rpx;
}

.button-take-order {
  height: 90rpx;
  line-height: 90rpx;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
  background: #fabe00;
}

.button-take-order:after {
  display: none;
}

.arrow_dir {
  display: flex;
  height: 70rpx;
  text-align: center;
  align-items: center;
  justify-content: center;
}

.arrow_dir image {
  width: 28rpx;
  height: 28rpx;
  padding: 20rpx;
  transition: transform 400ms;
}

.arrow_dir .icon-arrow-down {
  transform: rotate(90deg);
}

.arrow_dir .icon-arrow-up {
  transform: rotate(-90deg);
}

.arrow-button {
  height: 64rpx;
  padding: 0 20rpx;
  background: transparent;
  border: none;
  line-height: 62rpx;
}

.arrow-button::after {
  display: none;
}

.button-back {
  z-index: 2;
  font-size: 22rpx;
  position: absolute;
  top: 20rpx;
  right: 30rpx;
  background:transparent;
  border-radius: 4rpx;
}

.select-info .grid-col {
  margin-top: 30rpx;
}

.grid-col-tit {
  width: 80%;
}

.mue-content {
  padding-bottom: 140rpx;
}
.mue-content.iphoneX {
  padding-bottom: 208rpx;
}

.mue-header {
  display: flex;
  width: 750rpx;
  height: 70rpx;
  padding: 0 40rpx;
  color: #e2c361;
  font-size: 24rpx;
  text-align: center;
  align-items: center;
  background-color: #fff9e5;
}

.icon-face {
  width: 40rpx;
  height: 40rpx;
  margin-right: 15rpx;
}

.grid-col {
  float: none;
  display: block;
  width: 100%;
  line-height: 1;
  background-color: #fff;
}

/** cell-item**/

.cell-item {
  position: relative;
}

.cell-item-inner {
  display: flex;
  height: 100rpx;
  padding: 40rpx 0;
  margin: 0 40rpx;
  align-items: center;
  justify-content: space-between;
}

.cell-item .icon-link {
  width: 12rpx;
  height: 21rpx;
}

.cell-item-left {
  display: flex;
  color: #7c829d;
  font-size: 28rpx;
  line-height: 1;
  align-items: center;
  justify-content: space-between;
}
.recycle .cell-item-left{
  font-size: 28rpx;
  color:#292e3e;
}

.cell-item-left .label {
  margin-left: 20rpx;
  color: #cacee0;
  font-size: 26rpx;
}

.pink {
  color: #cacee0;
  font-size: 24rpx;
}

/** .grid-col-2 .input **/

.grid-col-2 .inline-list {
  display: flex;
  padding: 0 40rpx;
  align-items: center;
  justify-content: space-between;
  overflow: visible;
}

.grid-col-2 .input {
  display: inline-block;
  width: 305rpx;
  color: #292e3e;
  font-size: 30rpx;
  /* margin-top: 10rpx; */
  font-family: PingFang-SC-Medium;
  transform: translateY(-15rpx);
}

.recycle-cols {
  margin: 0 40rpx;
  /* border: 1rpx solid #cacee0; */
  /* border-radius: 6rpx; */
  position: relative;
}
.recycle-col {
  width: 33.33%;
  /* height: 80rpx; */
  font-size: 26rpx;
  font-weight: bold;
  color: #7c829d;
  box-sizing: border-box;
  display: inline-block;
  /* border: 1rpx solid #dde0ec; */
}
.recycle-col .inner{
  text-align: center;
  position: relative;
}
.recycle-col:first-child {
  border-radius: 6rpx 0 0 6rpx;
  /* border-right: 1rpx solid #fabe00;  */
}
.recycle-col:last-child {
  border-radius: 0 6rpx 6rpx 0;
  border-left: none; 
}
.recycle-col .inner view{
  /* display: inline-block; */
  overflow: visible;
  height: 70rpx;
  font-size: 28rpx;
  line-height: 59rpx;
  /* margin-left: 10rpx; */
  color: #7c829d;
}
.recycle-col.selected .inner::after{
  content: '';
  position: absolute;
  bottom: 0;
  width: 50rpx;
  height: 4rpx;
  background: #7c829d;
  left: 50%;
  transform: translateX(-50%);
}
.col-post .icon {
   width: 43rpx; 
  height: 43rpx;
  vertical-align: middle;
  /* margin-top: -5rpx; */
}

.col-visit .icon {
   width: 45rpx; 
  height: 43rpx;
  vertical-align: middle;
  /* margin-top: -5rpx; */
}
.col-fengchao .icon {
   width: 49rpx; 
  height: 43rpx;
  vertical-align: middle;
  /* margin-top: -5rpx; */
}

.recycle-col.selected {
  color: #fabe00;
  border-color: #fabe00;
}

/** picker **/

.grid-col-3 .cell-item-inner {
  border-bottom: 1rpx solid #eff0f6;
}
.cell-item-inner.nobb{
  border-bottom: none;
}
.cell-item-inner.nobb .cell-item-left{
  color: #292e3e;
  margin-left: 20rpx;
}
.cell-item-inner.nobb .cell-item-right .mue-button{
  color: #5498ff;
}

.picker .cell-item-left {
  color: #5498ff;
  font-size: 28rpx;
}

.picker-selected .cell-item-left {
  color: #292e3e;
}
.cell-item-left input{
  color: #292e3e;
  width: 100%;
}
.input-outer.cell-item-left{
  width: 100%;
}

/** footer **/

.mue-footer button {
  width: 670rpx;
  height: 90rpx;
  color: #cacee0;
  font-size: 32rpx;
  line-height: 90rpx;
  border-radius: 4rpx;
  background-color: #fff;
  border: 1rpx solid #cacee0;
}
.mue-footer button.now{
  background-color: #fabe00;
  border: 1rpx solid #fabe00;
  color: #fff;
}

.mue-footer button:after {
  display: none;
}

.grid-col-3 .fore1 .cell-item-inner {
  border-bottom: none;
  height: 0;
  padding: 20rpx 0;
}

.cell-item-right .mue-button {
  display: flex;
  height: 60rpx;
  padding: 0 20rpx;
  color: #cacee0;
  align-items: center;
}

.cell-item-right .mue-button text {
   margin-right: 10rpx; 
}

.cell-item-left .icon-query {
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
}
.recycle .cell-item-left .icon-query {
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
}

.grid-col-2 .form-control {
  position: relative;
  /* padding-bottom: 10rpx; */
  padding-left: 20rpx;
  margin-bottom: 10rpx;
  border: 1rpx solid #cacee0;
  border-radius: 4rpx;
  overflow: visible;
}
.grid-col-2 .form-control:first-child{
  margin-right: 10rpx;
}
.grid-col-2 .form-control:last-child{
  margin-left: 10rpx;
}

.grid-col-2 .form-control .label {
  display: inline-block;
  background: #fff;
  /* padding: 0 10rpx; */
  height: 26rpx;
  color: #cacee0;
  font-size: 26rpx;
  transition: all 300ms;
  transform: translateY(20rpx);
}

.grid-col-2 .form-control.mue-focus .label,
.grid-col-2 .form-control.has-value .label {
  /* color: #5498ff; */
  color: #cacee0;
  font-size: 22rpx;
  transform: translateY(-15rpx);
}

.input-ph {
  color: #cacee0;
}


