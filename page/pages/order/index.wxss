}  */
.more.hide{
  display: none;
}
.view-col-1 .remark{
  color: #7c829d;
  position: relative;
  overflow:visible;
}
.view-col-1 .acceptTime{
  color: #cacee0;
}

.p-img-wrapper {
  display: flex;
  float: left;
  width: 110rpx;
  height: 110rpx;
  border-radius: 4rpx;
  align-items: center;
  justify-content: center;
  background-color: #f8f8fa;
}

.p-img {
  width: 86rpx;
  height: 86rpx;
}

.p-desc {
  float: left;
  margin-left: 30rpx;
  margin-top: 22rpx;
  font-size: 0;
  line-height: 1;
}

.p-name {
  color: #292e3e;
  font-size: 28rpx;
}

.user-price {
  margin-top: 22rpx;
  color: #cacee0;
  font-size: 24rpx;
}

.detect-number {
  color: #ff5c7f;
}

.color-pink {
  color: #ff5c7f;
}

.view-col-3 {
  margin-bottom: 0;
}

.view-col-3 .view-col-content {
  line-height: 48rpx;
}

.button-sf {
  width: 670rpx;
  height: 86rpx;
  color: #fff;
  font-size: 32rpx;
  line-height: 80rpx;
  border-radius: 4rpx;
  background-color: #fabe00;
}

.button-sf::after {
  display: none;
}

.mue-footer{
  box-shadow: 0rpx -2rpx 12rpx 2rpx rgba(135, 142, 160, 0.1);
}
.button-take-order {
  height: 90rpx;
  line-height: 90rpx;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
  border-radius: 4rpx;
  background: #fabe00;
}

.button-take-order:after {
  display: none;
}


.mue-modal-orderinfo {
    z-index: 1000;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(0, 0, 0, .7)
}
.mue-alert {
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -300rpx;
    transform: translateY(-50%);
    width: 600rpx;
     padding: 0; 
    border-radius: 20rpx;
    background-color: #ffffff;
}
.mue-modal-orderinfo .mue-alert-title {
  padding-left: 40rpx;
  font-size: 36rpx;
  line-height: 140rpx;
  color: #fff;
  position: relative;
  background-image: linear-gradient(to right, #6dd8ff 0%, #64a6ff 100%);
}
.mue-alert-title .close{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 40rpx;
  right: 21rpx;
}
.mue-alert-title .close image{
  width: 22rpx;
  height: 22rpx;
  position: absolute;
  top: 19rpx;
  right: 19rpx;
}

.mue-modal-orderinfo .mue-alert-body {
  margin: 30rpx 0;
  text-align: center;
  line-height: 1.5;
  color: #999;
  font-size: 30rpx;
}
.mue-alert-body .info{
  color: #cacee0;
  padding: 20rpx 40rpx 60rpx 90rpx;
  position: relative;
  overflow: visible;
}
