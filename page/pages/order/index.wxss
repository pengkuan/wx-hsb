.mue-alert-body .info image.status{
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  top: 28rpx;
  left: 40rpx;
  z-index: 99;
}
.mue-alert-body .info view.status{
  position: absolute;
  width: 12rpx;
  height: 12rpx;
  left: 49rpx;
  top: 37rpx;
  background: #cacee0;
  border-radius: 50%;
}
.mue-alert-body .info.first{
  color: #5498ff;
}
.mue-alert-body .left{
  font-size: 32rpx;
  float: left;
}
.mue-alert-body .right{
  font-size: 24rpx;
  line-height: 48rpx;
  float: right;
}
.mue-modal-orderinfo .mue-alert-footer{
  line-height: 85rpx;
  width: 100%;
  border-top: 1rpx solid #f5f5f5;
  text-align: center;
  color: #3cc51f;
}

.mue-modal-orderinfo .evaluate-tip {
  color: #5498ff;
}
