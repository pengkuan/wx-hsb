page {
  background: #fff;
}

.order-head {
  z-index: 9;
  display: flex;
  width: 670rpx;
  margin: 20rpx auto 0;
  border-radius: 10rpx;
  background-image: linear-gradient(to right, #6dd8ff 0%, #64a6ff 100%);
}

.icon_order-state {
  width: 73rpx;
  height: 98rpx;
  margin: 50rpx 42rpx;
}

.order-state-cols {
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
}

.order-state-col {
  display: flex;
  width: 487rpx;
  color: #ffffff;
  align-items: center;
  justify-content: space-between;
}

.order-state-name {
  font-size: 32rpx;
}

.order-state-time {
  font-size: 24rpx;
}
.header{
  padding-bottom: 40rpx;
}
.fc-code{
  /* box-sizing: border-box; */
  display: flex;
  z-index: 1;
  width: 670rpx;
  top: 200rpx;
  margin: 0 auto;
  height: 100rpx;
  border-radius: 0 0 10rpx 10rpx;
  background: #fff;
  box-shadow: 0px 5px 10px 1px rgba(135, 142, 160, 0.1);
  
}
.fc-code .left{
  font-size: 26rpx;
  color: #7c829d;
  margin-left: 20rpx;
  float: left;
  width: 100rpx;
  line-height: 110rpx;
}
.fc-code .right{
  flex: 1;
  font-family: 'DINCondensedC';
  font-size: 54rpx;
  color: #adb2c8;
  float: right;
  text-align: right;
  margin-right: 20rpx;
  line-height: 110rpx;
}

.view-cols {
  background: #f9f9fb;
}
.view-cols.nearFc {
  padding-bottom: 130rpx;
}
.view-cols.nearFc.iphoneX{
  padding-bottom: 198rpx;
}
.view-col {
  padding: 0 40rpx;
  padding-bottom: 40rpx;
  margin-bottom: 20rpx;
  font-size: 0;
  background: #fff;
  /* overflow: visible; */
}

.view-col-title {
  padding-top: 30rpx;
  color: #292e3e;
  font-size: 28rpx;
}
.route .view-col-title {
  border-top: 1px solid #e7e8f0;
}

.view-col-content {
  padding-top: 20rpx;
  color: #7c829d;
  font-size: 26rpx;
  line-height: 32rpx;
  overflow: visible;
}

.view-col-1 .view-col-content {
  color: #7c829d;
  font-size: 26rpx;
  line-height: 48rpx;
}
.view-col-1 .route-li{
  padding-bottom: 20rpx;
  position: relative;
  padding-left: 50rpx;
  position: relative;
  overflow: visible;
  display: none;
}
.view-col-1 .route-li.showAll{
  display: block;
}
.route-li.first,.route-li.second{
  display: block;
}
