.about-us {
  width: 670rpx;
  height: 200rpx;
  margin: 0 auto;
  margin-top: 20rpx;
}
.version{
  position: absolute;
  top: 60rpx;
  left: 80rpx;
}
.version .logo{
  width: 120rpx;
  height: 120rpx;
}
.version .cont{
  display: inline-block;
  vertical-align: top;
  margin-left: 30rpx;
  padding-top: 20rpx;
  color: #fff;
  font-size: 32rpx;
  line-height: 32rpx;
}
.version .ver{
  margin-top: 20rpx;
  font-size: 26rpx;
  line-height: 26rpx;
}
.content .section{
  line-height: 130rpx;
  color: #7c829d;
  font-size: 30rpx;
  margin: 0 40rpx;
  padding: 0 10rpx;
  border-bottom: 1rpx solid #eff0f6;
  position: relative;
}
.content .section:last-child{
  border-bottom: none;
}
.content .section .goto{
  width: 12rpx;
  height: 21rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 10rpx;
}
.copy-right{
  position: absolute;
  bottom: 145rpx;
  width: 100%;
}
.copy-right view{
  font-size: 22rpx;
  color: #cacee0;
  text-align: center;
  width: 100%;
  line-height: 2;
}
.bottom-bg {
  width: 750rpx;
  height: 124rpx; 
  margin: 0 auto;
  position: absolute;
  bottom: 6rpx;
}