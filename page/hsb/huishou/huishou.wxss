.search-view-2 {
    width: 92%;
    height: 34px;
    margin: 16px auto;
}
.search-btn-2 {
    width: 90%;
    float: left;
    height: 34px;
    background: #fff;
    font-size: 14px;
    border-radius: 3px;
    position: relative;
    color: rgb(153, 153, 153);
}
.closeSearch-2 {
    width: 10%;
    float: right;
    text-align: right;
    line-height: 34px;
    font-size: 14px;
    color: rgb(153, 153, 153);
}
.search-input-2 {
    width: 97%;
    height: 100%;
    border: none;
    outline: none;
    padding-left: 3%;
    font-size: 12px;
}
.search-icon-2 {
    width: 34px;
    height: 34px;
    position: absolute;
    top: 8px;
    left: 0;
    text-align: center;
}


.search-result {
    width: 94%;
    height: 50px;
    margin: 0 auto;
    line-height: 50px;
    font-size: 16px;
    border-bottom: 1px solid #e1e1e1;
    color: #999;
    font-family: "微软雅黑";
}
.result-name {
    width: 70%;
    float: left;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
    font-size: 14px;
}
.result-maxprice {
    width: 30%;
    float: left;
    text-align: right;
    font-size: 14px;
}
.maxprice-text {
    display: block;
    width: 68%;
    float: left;
}
.maxprice-num {
    display: block;
    width: 32%;
    float: right;
}
.no-commodity {
    width: 100%;
    line-height: 50px;
    font-size: 14px;
    color: #999;
    text-align: center;
    background: #ececec;
}

#main {
  width: 94%;
  height: 30px;
  display: flex;
  margin: 0 auto;
}
.sousuo-text {
    width: 94%;
    margin: 0 auto;
    background: #fff;
    border-radius: 4px;
    text-align: center;
    font-size: 12px;
    color: #999;
}
.oops:nth-of-type(1) {
    flex-grow: 8;
    background: #fff;
    border-radius: 4px;
    height: 30px;
    line-height: 30px;
}
.oops:nth-of-type(2) {
    flex-grow: 1;
    text-align: right;
    height: 30px;
    line-height: 30px;
}
.main-view {
    width: 100%;
    padding: 14px 0;
    line-height: 30px;
    font-size: 12px;
    color: #999;
    display: flex;
}

.mun-view-body {
    display:flex;
    min-height:100vh;
    flex-direction:column;
    position:fixed;
    width:100%;
    top:0;
    left:0;
    height:100%;
}
.mun-view-1 {
    flex: 1;
    overflow-y: auto;
    background: #fff;
}
.pagesearch {
    flex: 1;
    overflow-y: auto;
    background: #fff;
}
.info-view {
    width: 100%;
    line-height: 50px;
    text-align: center;
    color: #999;
    font-size: 14px;
    font-family: "微软雅黑";
    border-top: 1px solid #f5f5f5;
}


.page-brand {
    width: 100%;
    overflow: hidden;
    position: fixed;
    top: 0;
    left: 0;
    background: #fff;
    flex: 1;
}

.page__bd {
    position: fixed;
    top:58px;
    left:0;
    width:100%;
    height: 100%;
}
.main-oop {
    position: fixed;
    top:58px;
    left:0;
    width:100%;
    height: 100%;
}
.mun-view-0 {
    background: #f5f5f5;
}

navigator {
    height:auto;
    width:auto;
    display:block;
}

