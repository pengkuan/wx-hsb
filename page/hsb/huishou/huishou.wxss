.color-f9be00 {
    color: #f9be00;
}
input {
    min-width : 240px;
}

.search-result-view {
    background: #fff;
    height: 100%;
}
.main{
    height            : 100%;
    display           : flex;
    flex-direction    : row;
}
.left{
    width     : 24%;
    float     : left;
    display   : flex;
    background-color: #fff;
    flex-grow : 2;
    font-size: 14px;
}
.right{
    width: 76%;
    float: left;
    background-color  : #fff;
    display           : flex;
    flex-grow : 5;
    font-size: 14px;
}
.item{
    line-height: 50px;
    height: 50px;
    font-size: 14px;
    font-family: "微软雅黑";
    overflow: hidden; text-overflow:ellipsis; white-space: nowrap;
}
.brand {
    text-align: center;
    background:#f6f6f6;
}
.right .item:hover {

    color : #666;
}
.page__bd .actives {
    color : #ff9300;
    border-left : 4px solid #ff9300;
    border-right-color: transparent;
}
.text-num {
    float: left;
    width: 14px;
    height: 14px;
    background: #d8dde0;
    line-height:14px;
    text-align: center;
    color: #fff;
    margin: 15px 10px 0 22px;
    border-radius: 2px;
    font-size: 12px;
    padding: 2px;
}
.navigator-view {
    padding-right:3%;
}
.navigator-view:nth-child(1) .text-num {
    background: #ff9300;
}
.navigator-view:nth-child(2) .text-num {
    background: #ffd302;
}
.navigator-view:nth-child(3) .text-num {
    background: #56b7e2;
}
.abc {
    color : #fff;
}
.xyz {
    color : #999;
}

/*品牌列表搜索 model*/
.search-view {
    width: 100%;
    overflow: hidden;
}
.sousuo-icon {
    width: 100%;
    height: 60px;
}
.search-btn {
    border-radius: 3px;
    padding: 0 10px;
    height: 32px;
    background: #fff;
    text-align: center;
    font-size: 12px;
    color: rgb(153, 153, 153);
}
.search-icon {
    width: 50px;
    margin-top: 7px;
    text-align: center;
}
.closeSearch {
    height: 34px;
}
