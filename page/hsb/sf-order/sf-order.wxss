view {
    font-family: "微软雅黑";
}
/*顺丰*/
.info-list {
    width: 100%;
    height: 50px;
    border-bottom: 1px solid #f1f1f1;
    font-size: 14px;
    background: #fff;
    display: flex;
}
.list-left {
    width: 68px;
    color: #666;
    display: flex;
    justify-content: space-between;
    -webkit-justify-content: space-between;
    margin-left: 6%;
    padding: 12px 0;
    padding-left: 20px;
    line-height: 24px;
}
.list-right {
    width: 76%;
    margin-left: 3%;
    float: left;
    padding: 12px 0;
    line-height: 24px;
    color: #999;
    flex: 1;
}
.list-right-addr {
    width: 100%;
    height: 50px;
    display: table-cell;
    vertical-align: middle;
}
.list-text {
    width: 100%;
    height: 50px;
    outline: none;
}
.info-name {
    line-height: 36px;
    height     : 36px;
    font-size  : 14px;
    width      : 94%;
    padding    : 0 3%;
    color      : #999;
}
.picker-view {
    width: 33.3333%;
    height: 200px;
    background: #fff;
    float: left;
}
.picker-title {
    width: 100%;
    height: 50px;
    line-height: 50px;
    background: #f1f1f1;
    text-align: center;
    position: relative;
    font-size: 16px;
}
.picker-list {
    font-size: 14px;
    line-height: 50px;
    text-align: center;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.picker-close {
    padding: 0 20px;
    color: #666;
    background: #f1f1f1;
    position: absolute;
    top: 0;
    right: 0;
}

.picker-body-0 {
    position: fixed;
    bottom: -250px;
    left: 0;
    width: 100%;
    height: 250px;
    z-index: 999999;
    -webkit-transition: all .4s linear;
    transition: all .4s linear;
    background: #fff;
}
.picker-body-1 {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 250px;
    z-index: 999999;
    -webkit-transition: all .4s linear;
    transition: all .4s linear;
    background: #fff;
}
.detailed-address {
    width: 90%;
    max-height: 200px;
    padding: 0 5%;
    overflow-y: auto;
}
.address-view {
    width: 100%;
    max-height: 250px;
    position: absolute;
    top: 50px;
    left: 0;
    z-index: 999;
    background: #f1f1f1;
}
.address-title {
    width: 100%;
    text-align: center;
    position: relative;
}

.sf-view {
    width: 100%;
    display: flex;
    flex-direction:column;
    height: 100%;
    min-height:100vh;
    position: fixed;
    top: 0;
    left: 0;
}
.sf-info-view {
    flex: 1;
    overflow-y: auto;
    background: #f5f5f5;
    width: 100%;
    overflow-y: auto;
}
.appointment-btn-view {
    width: 100%;
    height: 44px;
    padding: 20px 0;
}
.appointment-btn-true {
    width: 94%;
    height: 44px;
    line-height: 44px;
    text-align: center;
    background: #f9be00;
    color: #333;
    margin: 0 auto;
    font-size: 16px;
    border-radius: 4px;
    -webkit-transition: all .3s linear;
    transition: all .3s linear;
}
.appointment-btn-false {
    width: 94%;
    height: 44px;
    line-height: 44px;
    text-align: center;
    background: #ccc;
    color: #333;
    margin: 0 auto;
    font-size: 16px;
    border-radius: 4px;
    -webkit-transition: all .3s linear;
    transition: all .3s linear;
}
.popup-child-block,.popup-child-none {
    width: 100%;
    line-height: 40px;
    font-size: 16px;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate3d(-50%, -50%, 0);
    z-index: 4;
    color: #fff;
    text-align: center;
    -webkit-transition: all .4s linear;
    transition: all .4s linear;
}
.popup-child-block {
    opacity: 1;
}
.popup-child-none {
    opacity: 0;
}
.popup-child-text {
    padding: 10px 20px;
    background: #222;
    border-radius: 4px;
    font-family: "微软雅黑";
}




.mail-input-list {
    background-size: 12px;
    background-position: 10px 50%;
    font-family: "微软雅黑";
    color: #999;
    background: #fff;
    font-size: 14px;
    display: flex;
}
.mail-list-left {
    width: 68px;
    float: left;
    display: flex;
    justify-content: space-between;
    -webkit-justify-content: space-between;
    padding: 12px 0;
    line-height: 24px;
}
.mail-list-right {
    flex: 1;
    float: left;
    position: relative;
    padding: 12px 0;
}
.mail-input {
    margin-left: 12px;
    font-size: 14px;
    line-height: 24px;
    font-family: "微软雅黑";
    color: #999;
    width: 92%;
}
.mail-input-list-tel {
    background-size: 12px;
    background-position: 10px 50%;
    font-family: "微软雅黑";
    color: #999;
    height: 46px;
    line-height: 46px;
    background: #fff;
    font-size: 14px;
    display: flex;
}
.mail-list-left-tel {
    width: 68px;
    float: left;
    display: flex;
    justify-content: space-between;
    -webkit-justify-content: space-between;
}
.mail-list-right-tel {
    flex: 1;
    float: left;
    border-bottom: 1px solid #f5f5f5;
    position: relative;
    height: 46px;
}
.mail-input-tel {
    margin-left: 12px;
    font-size: 14px;
    line-height: 46px;
    height: 46px;
    font-family: "微软雅黑";
    color: #666;
    width: 100%;
}


.mail-icon {
    width   : 20px;
    height  : 20px;
    float: left;
    margin: 13px 3% 0 3%;
}
.post-popup {
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.3);
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
}
.post-child {
    width: 80%;
    background: #fff;
    position: fixed;
    top: 50%;
    left: 50%;
    overflow: hidden;
    transform: translate3d(-50%, -50%, 0);
    padding: 20px;
    border-radius: 4px;
}
.child-txt {
    line-height: 26px;
    font-size: 16px;
    color: #666;
}
.child-btn {
    width: 100%;
    height: 42px;
    line-height: 42px;
    text-align: center;
    margin-top: 20px;
    font-size: 14px;
    display: flex;
}
.child-btn-left {
    width: 50%;
    background: #fff;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    border: 1px solid #e6e6e6;
    float: left;
    flex-grow: 1;
}
.child-btn-right {
    width: 50%;
    background: #f9be00;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    border: 1px solid #f9be00;
    float: left;
    color: #333;
    flex-grow: 1;
}
