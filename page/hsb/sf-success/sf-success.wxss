.sf-success-view {
    width: 100%;
    height: 100%;
    background: #fff;
    display: -webkit-box;
    -webkit-box-pack: center;
    -webkit-box-align: center;
    -webkit-box-orient: vertical;
    text-align: center;
}
.sf-success-icon {
    width: 40px;
    height: 40px;
    display: -webkit-box;
}
.sf-text-0 {
    margin-top: 10px;
    color: #f9be00;
}
.order-success-btn {
    width: 100%;
    position: fixed;
    bottom: 30px;
    left: 0;
    width: 100%;
    line-height: 46px;
    font-size: 16px;
    text-align: center;
}
.list-btn {
    width: 45.5%;
    height: 46px;
    float: left;
    margin: 0 3%;
    border-radius: 4px;
    border: 1px solid #f9be00;
}
.sf-btn {
    width: 45.5%;
    height: 46px;
    float: left;
    background: #f9be00;
    border-radius: 4px;
    border: 1px solid #f9be00;
}